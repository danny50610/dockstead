function php74() {
    sudo update-alternatives --set php /usr/bin/php7.4
}

function php80() {
    sudo update-alternatives --set php /usr/bin/php8.0
}

function php81() {
    sudo update-alternatives --set php /usr/bin/php8.1
}

function php82() {
    sudo update-alternatives --set php /usr/bin/php8.2
}
